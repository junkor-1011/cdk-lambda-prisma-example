{
  "name": "prisma-lambda-app",
  "private": true,
  "version": "0.1.0",
  "bin": {
    "prisma-lambda-app": "bin/prisma-lambda-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "typecheck": "tsc --noEmit",
    "unit": "jest",
    "test": "pnpm typecheck && pnpm unit",
    "cdk": "cdk",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "format": "prettier --write . '!.tmp'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "18.11.9",
    "@types/prettier": "2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "aws-cdk": "2.50.0",
    "esbuild": "^0.15.14",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-define-config": "^1.12.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.50.0",
    "constructs": "^10.1.162",
    "source-map-support": "^0.5.21"
  }
}
